<rvx>
	<platform>
		<name>test_hw_im2col</name>
		<status>described</status>
		<spec>
			<define>
				<name>sram_size</name>
				<value>64kB</value>
			</define>
			<define>
				<name>num_lock_user</name>
				<value>1</value>
