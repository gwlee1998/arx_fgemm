			</define>
			<define>
				<name>include_slow_dram</name>
				<value type="bool">True</value>
			</define>
			<define>
				<name>num_led_checker</name>
				<value>1</value>
			</define>
		</spec>

		<ip_instance>
			<name>i_main_core</name>
			<library_name>rvc_orca</library_name>
    		</ip_instance>
		<ip_instance>
			<name>i_IM2COL_ENGINE</name>
			<library_name>user_slaveif_axi_clkin</library_name>
			<parameter>
				<id>SIZE_OF_MEMORYMAP</id>
				<value>132kB</value>
			</parameter>
			<parameter>
				<id>BW_AXI_TID</id>
				<value>`REQUIRED_BW_OF_SLAVE_TID</value>
			</parameter>
			<parameter>
				<id>BW_ADDR</id>
				<value>32</value>
			</parameter>
			<parameter>
				<id>BW_DATA</id>
				<value>32</value>
			</parameter>
		</ip_instance>
